#!/bin/bash

# Simplified Spring Boot staged application launcher
# This is a test fixture - only includes the minimal JAR needed for testing

APP_HOME="$(cd "$(dirname "$0")/.." && pwd)"
CLASSPATH=$APP_HOME:$APP_HOME/lib/spring-boot-3.0.0.jar

# Launch the application
exec $JAVA_HOME/bin/java -cp "$CLASSPATH" io.pivotal.SimpleJava "$@"
